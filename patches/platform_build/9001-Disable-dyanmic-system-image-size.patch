From: AndroPlus <mail@andro.plus>
Date: Sat, 1 Dec 2018 15:59:57 +0900
Subject: [PATCH 1/1] Disable dyanmic system image size

Change-Id: Ic62ca023c07565bf6cba3fae751718fb695202af
Signed-off-by: AndroPlus <mail@andro.plus>
---
 target/board/treble_common.mk   | 2 +-
 target/product/treble_common.mk | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/target/board/treble_common.mk b/target/board/treble_common.mk
index 3df2e9360..b2b950ed0 100644
--- a/target/board/treble_common.mk
+++ b/target/board/treble_common.mk
@@ -37,7 +37,7 @@ TARGET_USERIMAGES_SPARSE_EXT_DISABLED := false
 TARGET_USES_MKE2FS := true
 
 # Enable dyanmic system image size and reserved 64MB in it.
-BOARD_SYSTEMIMAGE_PARTITION_RESERVED_SIZE := 67108864
+# BOARD_SYSTEMIMAGE_PARTITION_RESERVED_SIZE := 67108864
 
 # Generic AOSP image always requires separate vendor.img
 TARGET_COPY_OUT_VENDOR := vendor
diff --git a/target/product/treble_common.mk b/target/product/treble_common.mk
index a2a6b80a5..d1bfa7b04 100644
--- a/target/product/treble_common.mk
+++ b/target/product/treble_common.mk
@@ -24,7 +24,7 @@ $(call inherit-product, $(SRC_TARGET_DIR)/product/aosp_base.mk)
 $(call inherit-product, $(SRC_TARGET_DIR)/product/telephony.mk)
 
 # Enable dynamic partition size
-PRODUCT_USE_DYNAMIC_PARTITION_SIZE := true
+# PRODUCT_USE_DYNAMIC_PARTITION_SIZE := true
 
 # Split selinux policy
 PRODUCT_FULL_TREBLE_OVERRIDE := true
